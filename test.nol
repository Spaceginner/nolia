fnc println |n: %i 64|
asm {
    syscall println;
}

fnc add |a: %i 64, b: %i 64| -> %i 64
asm {
    syscall add;
}

fnc fib || -> ! {
    let a: %i 64 = 0;
    let b: %i 64 = 1;
    #loop {
        println(b);
        let c: %i 64 = add(a, b);
        a = b;
        b = c;
    }
}

fnc entry || {
    fib()
}
